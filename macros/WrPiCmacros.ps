%currentpoint translate
% Millstone Hill 4.16667 -4.16667 scale
%5.5555555 -5.5555555 scale % Aber values for 400 dpi printer
/WrPiCdict 20 dict def WrPiCdict begin
/Times-Roman findfont 9 scalefont setfont
/str 20 string def
/m {moveto} def
/l {lineto} def
/begincurve {0 5 translate /nt cvlit exch def /dt cvlit exch def /vc cvlit exch def newpath} def
/fillbump {fill newpath} bind def 
%
/text {gsave matrix defaultmatrix setmatrix 
/Times-Roman findfont 5 scalefont setfont 
vc str cvs 0 setgray show grestore} def % text
%
/bdef {bind def} def
/bumpstart {} def
/bumpend {/rmean cvlit exch def /rlast cvlit exch def /rfirst cvlit exch def } def
%
/endcurve {
rfirst -20 add 0 moveto text newpath
rmean 0.5 sub -0.5 moveto nt -1 add dt mul rmean add 1 add -0.5 lineto 
1 setlinewidth [1 dt 1 sub] 0.5 setdash stroke
0 exch translate pop} def
/oldstroke {stroke} bind def
/filetrailer{/stroke{oldstroke}def end}def
end % WrPiCdict
% Initialize the PS coordinate system to same as in PiCTeX:
/initialize{
WrPiCdict begin
72 25.4 div 72 25.4 div scale % to 1mm units
xunit yunit scale % to final units
xref -1 mul yref -1 mul translate % corresponding to point spec.
newpath xmin ymin moveto xmin ymax lineto 
        xmax ymax lineto xmax ymin lineto 
closepath reversepath clip newpath 
} def % initialize
