/tdPiCdict 20 dict def tdPiCdict begin
/box {gsave setgray 
      newpath 2 index 2 copy moveto 0.5 add lineto exch
      2 copy 0.5 add lineto lineto closepath 
	  gsave fill grestore 0 setgray stroke grestore} def
/longbox {gsave setgray 
      newpath 2 index 2 copy -0.5 add moveto 10 add lineto exch
      2 copy 10 add lineto -0.5 add lineto closepath 
	  gsave fill grestore 0 setgray stroke grestore} def
/;PS_PHA {aload pop 0 box} bind def
/;PS_PHAN {aload pop gsave 0 -0.5 translate 0 box grestore} def
/;PS_EMPTY {aload pop newpath 2 index moveto exch lineto stroke} def %	nothing happening
/;PS_MATCHFIL {aload pop gsave 0 0.5 translate 0.5 box grestore} def % matched filter on (for all channels)
/;PS_CALON {aload pop 0.9 longbox} def % noise injection on (for all channels)
/;PS_ON {aload pop 0.5 box} def % channel sampling on
/;PS_BUFILT {pop} def
/;PS_LIFILT {pop} def
/;PS_LIFILT {pop} def
/;PS_SETFREQ {pop} def
/;PS_SETADCINT {pop} def
/oldstroke {stroke} bind def
/stroke {gsave initmatrix 0 setlinewidth oldstroke grestore} bind def
/filetrailer{
/stroke{oldstroke}def 
gsave [0.1 0.9] -0.05 setdash xmin xdenseinterval xmax 
{newpath dup -0.1 moveto 10 lineto stroke} for  
grestore end}def 
end % tdPiCdict
/initialize{
tdPiCdict begin
0.01 setlinewidth 
72 25.4 div 72 25.4 div scale % to 1mm units
xunit yunit scale % to final units
xref -1 mul yref -1 mul translate % corresponding to point spec.
newpath xmin ymin moveto xmin ymax lineto xmax ymax lineto 
        xmax ymin lineto 
closepath
reversepath 
clip 
newpath 
} def

