SET-COR-APB 11,20       % 21 SAMPLES IN A RANGECELL
SET-COR-APB 10,26       % 28 ACF'S, 1C + 5B + 22S PER CHANNEL
SET-COR-APB 9,15        % 16 LAGS PER ACF
SET-COR-APB 8,89        % 90 POWER POINTS PER CHANNEL
SET-COR-APB 6,3         % 4 CHANNELS OF LONGPULSE DATA
SET-COR-APB 5,7         % 8 LAGS PER ROW IN FIRST MP, ALSO NR OF CAL ROWS
SET-COR-APB 4,3         % 4 LAGS PER ROW IN SECOND MP, ALSO LAG INCR
SET-COR-APB 3,77        % CALCULATE 78 MATRIX ROWS
SET-COR-APB 2,2         % MATRIX LAG INCREMENT
SET-CORR-APM 15,16      % 16 LAGS PER ACF
             %%% RESULT MEMORY MAP %%%
%%%%%%%%%%%%%%% 000-623   MULTIPULSE                 SIGNAL      I
%%%%%%%%%%%%%%% 624-713   LONG POWER PROFILE         SIGNAL
%%%%%%%%%%%%%%% 714-777   MULTIPULSE + POWER PROFILE BACKGROUND  I
SET-CORR-APM 13,778%905   MULTIPULSE + POWER PROFILE CALIBRATION I
%%%%%%%%%%%%%%% 906-1217  MULTIPULSE                 SIGNAL      II
%%%%%%%%%%%%%%% 1218-1307 SHORT POWER PROFILE        SIGNAL
%%%%%%%%%%%%%%% 1308-1339 MULTIPULSE + POWER PROFILE BACKGROUND  II
SET-CORR-APM 11,1340%1403 MULTIPULSE + POWER PROFILE CALIBRATION II
SET-CORR-APM 10,1404%1835 LONGPULSE BACKGROUND + SIGNAL
 SET-CORR-APM 9,1836%1899 LONGPULSE CALIBRATION
SET-CORR-APM 0,1        % RESULT MEMORY INCREMENT
SET-CORR-DATAIO 1901    % SEE ABOVE
% NOTE: WINDOW NUMBERS AT RT-GRAPH ARE SHIFTED BY 1
SYNC -60
SET-BUFFER-MEM 2,226    % MULTIPULSE I + POW PROF AT CH2, 226 SAMPLES
SET-BUFFER-MEM 7,452    % MULTIPULSE II + POW PROF AT CH7, 211 SAMPLES
